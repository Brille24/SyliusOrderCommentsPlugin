{
    "name": "sylius/order-comments-plugin",
    "type": "sylius-plugin",
    "description": "Order comments feature for Sylius.",
    "license": "proprietary",
    "require": {
        "php": "^7.1",

        "sylius/sylius": "^1.0.0@beta",
        "simple-bus/symfony-bridge": "^4.1",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.4",
        "simple-bus/doctrine-orm-bridge": "^4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^6.0",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^0.3",
        "friends-of-behat/cross-container-extension": "^0.2",
        "friends-of-behat/performance-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^0.2.1",
        "friends-of-behat/variadic-extension": "^0.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\OrderCommentsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sylius\\OrderCommentsPlugin\\": "tests/"
        }
    }
}
