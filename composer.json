{
    "name": "sylius/order-comments-plugin",
    "type": "sylius-plugin",
    "description": "Order comments feature for Sylius.",
    "license": "proprietary",
    "require": {
        "php": "^7.1",

        "sylius/sylius": "~1.3",
        "simple-bus/symfony-bridge": "^5.1",
